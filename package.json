{
  "name": "ky-extra",
  "version": "0.0.0",
  "description": "Small, zero-dep plugins and utilities for ky HTTP client: auth, smart retry, and portable in-memory cache.",
  "type": "module",
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fayez-kaabi/ky-extra.git"
  },
  "bugs": {
    "url": "https://github.com/fayez-kaabi/ky-extra/issues"
  },
  "homepage": "https://github.com/fayez-kaabi/ky-extra#readme",
  "sideEffects": false,
  "engines": {
    "node": ">=18.18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "keywords": [
    "ky",
    "fetch",
    "http",
    "plugin",
    "retry",
    "cache",
    "auth",
    "esm",
    "browser",
    "deno",
    "bun"
  ],
  "peerDependencies": {
    "ky": ">=0.33.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@changesets/cli": "^2.27.9",
    "@changesets/changelog-github": "^0.5.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "ky": ">=0.33.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2",
    "@vitest/coverage-v8": "^2.1.2"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}


